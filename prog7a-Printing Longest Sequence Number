#include<iostream>
using namespace std;
int max(int x,int y)
{
    return (x>y)?x:y;
}
void lcs(string x,string y,int m,int n)
{
    int l[m+1][n+1];
    int i,j;
        for(i=0;i<=m;i++)
            {
                for(int j=0;j<n;j++)
                {
                    if(i==0||j==0)
                        l[i][j]=0;
                    else if(x[i-1]==y[j-1])
                        l[i][j]=l[i-1][j-1]+1;
                    else
                        l[i][j]=max(l[i-1][j],l[i][j-1]);
                }
            }
    
int index=l[m][n];
char lcs[index+1];
lcs[index]='\0';
 i = m; j = n;
while (i > 0 && j > 0)
    {
        if (x[i-1] == y[j-1])
        {
            lcs[index-1] = x[i-1];
            i--; j--; index--;
        }
        else if (l[i-1][j] > l[i][j-1])
        i--;
        else
        j--;
        
    }
while(lcs[i]!='\0')
cout<<lcs[i++];
}
int main()
{
    string s1,s2;
    cout<<"enter 1 string";
    cin>>s1;
    cout<<"enter 2 string";
    cin>>s2;
    int m,n;
    m=s1.length();
    n=s2.length();
    lcs(s1,s2,m,n);
}
